{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/projets/outils/MaMaCollecte/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/projets/outils/MaMaCollecte/:path*",
      "destination": "/projets/outils/MaMaCollecte/:path*"
    },
    {
      "source": "/assets/:path*",
      "destination": "/assets/:path*"
    },
    {
      "source": "/",
      "destination": "/MaMaCollecte.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
